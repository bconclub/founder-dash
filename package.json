{
  "name": "proxe-platform",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "npx concurrently -n \"wc,proxe\" -c \"cyan,green\" \"npm run dev:windchasers\" \"npm run dev:proxe\"",
    "dev:master": "cd master/agent && npm run dev",
    "dev:windchasers": "cd windchasers/agent && npm run dev",
    "dev:proxe": "cd proxe/agent && npm run dev",
    "build": "npm run build:windchasers && npm run build:proxe",
    "build:master": "cd master/agent && npm run build",
    "build:windchasers": "cd windchasers/agent && npm run build",
    "build:proxe": "cd proxe/agent && npm run build",
    "sync": "bash scripts/sync-all-brands.sh",
    "kill-ports": "bash scripts/kill-build-ports.sh"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
